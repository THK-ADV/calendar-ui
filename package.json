{
  "name": "calendar-ui",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "lint": "eslint src",
    "fmt": "prettier --write .",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@rollup/plugin-json": "^6.1.0",
    "@smui-extra/autocomplete": "^8.0.0-beta.0",
    "@smui/button": "^8.0.0-beta.0",
    "@smui/card": "^8.0.0-beta.0",
    "@smui/checkbox": "^8.0.0-beta.0",
    "@smui/common": "^8.0.0-beta.0",
    "@smui/dialog": "^8.0.0-beta.0",
    "@smui/drawer": "^8.0.0-beta.0",
    "@smui/list": "^8.0.0-beta.0",
    "@smui/textfield": "^8.0.0-beta.0",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-node": "^5.0.1",
    "@sveltejs/kit": "^2.8.1",
    "@sveltejs/vite-plugin-svelte": "^4.0.1",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.0",
    "globals": "^15.12.0",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.8",
    "smui-theme": "^8.0.0-beta.0",
    "svelte": "^5.2.7",
    "svelte-check": "^4.0.9",
    "svelte-eslint-parser": "^0.43.0",
    "svelte-fullcalendar": "^3.0.0",
    "tslib": "^2.4.1",
    "typescript": "^5.5.0",
    "typescript-eslint": "^8.15.0",
    "vite": "^5.4.11",
    "vitest": "^1.2.0"
  },
  "type": "module",
  "dependencies": {
    "@fullcalendar/core": "^6.1.11",
    "@fullcalendar/daygrid": "^6.1.11",
    "@fullcalendar/interaction": "^6.1.11",
    "@fullcalendar/timegrid": "^6.1.11",
    "svelte-i18n": "^4.0.1"
  }
}
